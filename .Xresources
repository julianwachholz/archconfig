!!! Mouse
Xcursor.theme: Breeze_Snow
!Xcursor.size:

!!! Fonts
Xft.dpi: 192
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

!!! Rofi
rofi.font: Fira Code 20
rofi.color-enabled: true
! Property Name     BG       Border   Separator
rofi.color-window:  base01,  base01,  base00

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal: base01,   base05,  base01,  base01,  base07
rofi.color-active: base01,   base0D,  base01,  base01,  base0D
rofi.color-urgent: base01,   base08,  base01,  base01,  base08
rofi.separator-style: solid

!!! urxvt
URxvt.termName: xterm-256color
URxvt*saveLines: 10000
URxvt.scrollBar: false
URxvt.buffered: true
URxvt.borderless: true
URxvt.dynamicColors: on
URxvt.urgentOnBell: true
URxvt*iso14755: false
URxvt*iso14755_52: false
URxvt*imLocale: en_US.UTF-8

URxvt*font: xft:Fura Code Nerd Font:style=Regular,size=20
URxvt*xftAntialias: true
URxvt.dep‚êäth: 32
URxvt.background: base00
URxvt.transparent: false
URxvt.fading: 0

URxvt.perl-ext: default,url-select
URxvt.url-select.launcher: google-chrome-stable
URxvt.url-select.underline: true
URxvt.keysym.M-u: perl:url-select:select_next
!URxvt.keysym.M-c: perl:clipboard:copy
!URxvt.keysym.M-v: perl:clipboard:paste

